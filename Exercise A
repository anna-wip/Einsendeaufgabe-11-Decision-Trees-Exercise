if age = "<=30" and student = "no" then buys_computer = "no"
else age = "<=30" and student = "yes" then buys_computer = "yes"
else age = "31...40" then buys_computer = "yes"
else age = ">40" and credit_rating = "excellent" then buys_computer = "yes"
else age = "<=30" and credit_rating = "fair" then buys_computer = "no"
